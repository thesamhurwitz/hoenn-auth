    "cross-env": "^7.0.3",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^2.0.15",
    "prettier": "^1.17.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0"
  }
