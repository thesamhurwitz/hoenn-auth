    "@types/node": "^12.0.4",
    "body-parser": "^1.19.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^14.2.0",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.4",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "typescript": "^4.6.2",
    "winston": "^3.4.0"
